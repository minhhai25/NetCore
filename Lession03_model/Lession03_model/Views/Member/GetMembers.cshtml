<h2>Danh sách thành viên</h2>
<table>
    <tr>
        <th>Mã sinh viên</th>
        <th>Username</th>
        <th>phone</th>
        <th>Password</th>
        <th>Email</th>
    </tr>
    @foreach (var m in ViewBag.member)
    {
        <tr>
            <td>@m.MemberId</td>
            <td>@m.UserName</td>
            <td>@m.password</td>
            <td>@m.Email</td>
            <td>@m.Phone</td>
            <td>
               <a class="btn btn-primary" asp-action="Edit" asp-route-id="@m.MemberId">Edit</a>
                <a class="btn btn-primary" asp-action="Delete" asp-route-id="@m.MemberId">Delete</a>
                <a class="btn btn-primary" asp-action="Detail" asp-route-id="@m.MemberId">Detail</a>
            </td>

        </tr>
    }
</table>
